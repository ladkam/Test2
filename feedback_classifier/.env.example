# Google AI API Key - Get from https://aistudio.google.com/apikey
GOOGLE_API_KEY=your_api_key_here

# Database (PostgreSQL with pgvector)
DATABASE_URL=postgresql://user:password@localhost:5432/feedback_db

# Optional: Use SQLite for local development
USE_SQLITE=true
SQLITE_PATH=./feedback.db

# Classification settings
EMBEDDING_MODEL=gemini-embedding-001
CLASSIFICATION_MODEL=gemini-2.5-flash
EMBEDDING_DIMENSIONS=768
